{
  "name": "apexobj",
  "version": "0.8.2",
  "description": "Apex Legends data in Object form. Synchronous, performant and completely local - has no runtime dependencies and doesn't query mozambiquehe.re or any other external APIs.",
  "main": "./dist/index.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "watch": "nodemon ./dist/index.js",
    "test": "npx mocha",
    "test:watch": "npx mocha --watch",
    "build": "npx tsc --build --clean",
    "build:watch": "npx tsc --watch"
  },
  "keywords": [
    "apex",
    "apex legends",
    "respawn",
    "mozambique"
  ],
  "author": "Dan Foy",
  "license": "ISC",
  "homepage": "https://danfoy.github.io/ApexObj",
  "repository": {
    "type": "git",
    "url": "https://github.com/danfoy/ApexObj.git"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^20.4.8",
    "chai": "^4.3.4",
    "chai-like": "^1.1.1",
    "chai-things": "^0.2.0",
    "mocha": "^10.2.0",
    "mockdate": "^3.0.5",
    "nodemon": "^2.0.15",
    "typescript": "^5.1.6"
  }
}
